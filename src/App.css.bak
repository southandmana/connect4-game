.app {
  text-align: center;
  padding: 10px;
  background: #330000; /* Solid color instead of gradient */
  border-radius: 20px;
  border: 3px solid #ff4500;
  position: relative;
  overflow: hidden;
  min-height: 100vh;
  max-width: 100vw;
  box-sizing: border-box;
  
  /* Performance optimizations */
  will-change: auto;
  transform: translateZ(0);
  backface-visibility: hidden;
}

/* Removed heavy background effects for performance */

/* Removed flames keyframes for performance */

.app > * {
  position: relative;
  z-index: 2;
}

.game-header {
  margin-bottom: 20px;
  min-height: 20px;
}

.room-code {
  background: #8b0000; /* Solid color instead of gradient */
  color: #ffccaa;
  padding: 15px 20px;
  border-radius: 15px;
  margin-bottom: 20px;
  font-weight: bold;
  border: 2px solid #ff6b35;
  font-family: 'Fredoka', 'Comic Sans MS', 'Marker Felt', 'Bradley Hand', cursive, sans-serif;
}

.code-label {
  font-size: 1rem;
  margin-bottom: 10px;
  opacity: 0.9;
}

.code-display {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 15px;
  margin-bottom: 10px;
}

.room-code .code {
  font-family: monospace;
  font-size: 2rem;
  letter-spacing: 3px;
  background: rgba(255, 255, 255, 0.2);
  padding: 10px 20px;
  border-radius: 10px;
  border: 2px solid rgba(255, 255, 255, 0.3);
  font-weight: bold;
}

.copy-button {
  padding: 8px 15px;
  font-size: 0.9rem;
  background: #ff4500; /* Solid color instead of gradient */
  color: white;
  border: 2px solid #ffaa80;
  border-radius: 20px;
  cursor: pointer;
  transition: all 0.3s;
  font-family: 'Fredoka', 'Comic Sans MS', 'Marker Felt', 'Bradley Hand', cursive, sans-serif;
  font-weight: bold;
}

.copy-button:hover {
  background: #ff6b35; /* Solid hover color */
  transform: translateY(-2px);
}

.waiting {
  font-size: 0.9rem;
  margin-top: 5px;
  /* Removed pulse animation for performance */
}

/* Removed pulse keyframes - using static opacity instead */
.waiting { opacity: 0.8; }

.players-info {
  display: flex;
  justify-content: center;
  gap: 50px;
  margin-bottom: 20px;
}

.player-info {
  display: flex;
  align-items: center;
  gap: 10px;
  padding: 12px 20px;
  border-radius: 25px;
  background: #2d0000; /* Solid color instead of gradient */
  border: 2px solid #ff6b35;
  transition: all 0.3s;
  font-family: 'Fredoka', 'Comic Sans MS', 'Marker Felt', 'Bradley Hand', cursive, sans-serif;
}

.player-info.active {
  background: #8b0000; /* Flat active background */
  transform: scale(1.1);
  /* Flat design - no shadows */
  transition: transform 0.3s ease;
}

/* Removed playerPulse keyframes for performance */

.player-name {
  font-weight: bold;
  color: #ffccaa;
  /* Flat design - no text shadow */
}

.game-info {
  margin-bottom: 20px;
  font-size: 1.3rem;
  font-weight: bold;
  color: #ffccaa;
  font-family: 'Fredoka', 'Comic Sans MS', 'Marker Felt', 'Bradley Hand', cursive, sans-serif;
  /* Flat design - no text shadow */
}

.current-turn {
  font-size: 1.4rem;
  color: #ff6b35;
  /* Flat design - no text shadows */
}

/* Removed turnGlow keyframes for performance */

.player-indicator {
  display: inline-block;
  width: 30px;
  height: 30px;
  border-radius: 50%;
  vertical-align: middle;
}

.player-indicator.red {
  background: #ff4757;
  /* Flat design - no shadow */
}

.player-indicator.yellow {
  background: #ffd700;
  /* Flat design - no shadow */
}

.winner-message,
.draw-message {
  font-size: 1.8rem;
  font-weight: bold;
  color: #ff6b35;
  font-family: 'Fredoka', 'Comic Sans MS', 'Marker Felt', 'Bradley Hand', cursive, sans-serif;
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 15px;
  text-shadow: 
    0 0 10px #ff4500,
    0 0 20px #ff4500,
    2px 2px 4px rgba(0, 0, 0, 0.8);
  /* Static victory text for performance */
  
}

@keyframes victoryFlame {
  0% { 
    
    transform: scale(1);
  }
  100% { 
    
    transform: scale(1.05);
  }
}

.game-controls {
  display: flex;
  justify-content: center;
  gap: 15px;
  margin-top: 20px;
}

.reset-button,
.mute-button {
  padding: 15px 35px;
  font-size: 1.2rem;
  font-weight: bold;
  color: white;
  background: linear-gradient(145deg, #ff4500 0%, #8b0000 50%, #330000 100%);
  border: 3px solid #ff6b35;
  border-radius: 25px;
  cursor: pointer;
  transition: all 0.3s;
  font-family: 'Fredoka', 'Comic Sans MS', 'Marker Felt', 'Bradley Hand', cursive, sans-serif;
  
  box-shadow: 
    0 6px 12px rgba(255, 69, 0, 0.4),
    inset 0 2px 0 rgba(255, 140, 0, 0.2),
    inset 0 -2px 0 rgba(139, 0, 0, 0.8);
  position: relative;
  overflow: hidden;
}

.mute-button {
  background: linear-gradient(145deg, #ff6b35 0%, #ff4500 50%, #8b0000 100%);
}

.reset-button:hover,
.mute-button:hover {
  transform: translateY(-3px) scale(1.05);
  box-shadow: 
    0 8px 16px rgba(255, 69, 0, 0.6),
    inset 0 2px 0 rgba(255, 140, 0, 0.3),
    inset 0 -2px 0 rgba(139, 0, 0, 0.8);
  animation: buttonFlame 0.5s ease-in-out;
}

.reset-button:active,
.mute-button:active {
  transform: translateY(-1px) scale(1.02);
  box-shadow: 
    0 4px 8px rgba(255, 69, 0, 0.4),
    inset 0 2px 0 rgba(255, 140, 0, 0.1),
    inset 0 -2px 0 rgba(139, 0, 0, 0.9);
}

@keyframes buttonFlame {
