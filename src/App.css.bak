@keyframes charactersFloat {
  0% { transform: translateY(0px) rotate(0deg); opacity: 0.7; }
  50% { transform: translateY(-10px) rotate(2deg); opacity: 0.9; }
  100% { transform: translateY(-5px) rotate(-1deg); opacity: 0.8; }
}

.app {
  position: relative;
}

/* Floating fire particles */
.fire-particle {
  position: absolute;
  width: 6px;
  height: 6px;
  background: radial-gradient(circle, #ff4500, #ff6b35);
  border-radius: 50%;
  pointer-events: none;
  /* Removed floating animation for performance */
  opacity: 0.8;
}

.fire-particle::before {
  content: '';
  position: absolute;
  width: 100%;
  height: 100%;
  background: radial-gradient(circle, #ffd700, transparent);
  border-radius: 50%;
  /* Static glow for performance */
  opacity: 0.9;
}

@keyframes floatUp {
  0% {
    transform: translateY(100vh) translateX(0px) scale(1);
    opacity: 0;
  }
  10% {
    opacity: 1;
  }
  90% {
    opacity: 1;
  }
  100% {
    transform: translateY(-100px) translateX(50px) scale(0);
    opacity: 0;
  }
}

@keyframes flicker {
  0% { opacity: 0.8; transform: scale(1); }
  100% { opacity: 1; transform: scale(1.2); }
}

/* Background Characters */
.background-characters {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  pointer-events: none;
  z-index: 0;
}

.character {
  position: absolute;
  opacity: 0.6;
  filter: drop-shadow(0 0 10px rgba(255, 69, 0, 0.5));
}

/* Character 1 - Top Left, floating */
.char-1 {
  top: 10%;
  left: 5%;
  width: 80px;
  height: 80px;
  /* Removed complex animations for performance */
}

/* Character 2 - Top Right, bouncing */
.char-2 {
  top: 15%;
  right: 8%;
  width: 100px;
  height: 100px;
  /* Removed complex animations for performance */
}

/* Character 3 - Bottom Left, walking */
.char-3 {
  bottom: 10%;
  left: 10%;
  width: 120px;
  height: 120px;
  /* Removed complex animations for performance */
}

/* Character 4 - Middle Right, spinning */
.char-4 {
  top: 45%;
  right: 5%;
  width: 90px;
  height: 90px;
  /* Removed complex animations for performance */
}

/* Character 5 - Bottom Right, dancing */
.char-5 {
  bottom: 15%;
  right: 10%;
  width: 110px;
  height: 110px;
  /* Removed complex animations for performance */
}

@keyframes floatUpDown {
  0%, 100% { transform: translateY(0px); }
  50% { transform: translateY(-20px); }
}

@keyframes rotate {
  0% { transform: rotate(0deg); }
  100% { transform: rotate(360deg); }
}

@keyframes bounce {
  0%, 100% { transform: translateY(0px) scaleY(1); }
  50% { transform: translateY(-30px) scaleY(0.95); }
}

@keyframes sway {
  0%, 100% { transform: translateX(0px) rotate(0deg); }
  25% { transform: translateX(-10px) rotate(-5deg); }
  75% { transform: translateX(10px) rotate(5deg); }
}

@keyframes walkAcross {
  0% { left: -150px; transform: scaleX(1); }
  49% { transform: scaleX(1); }
  50% { left: calc(100% + 150px); transform: scaleX(-1); }
  99% { transform: scaleX(-1); }
  100% { left: -150px; transform: scaleX(1); }
}

@keyframes bobbing {
  0%, 100% { transform: translateY(0px); }
  50% { transform: translateY(-5px); }
}

@keyframes spin {
  0% { transform: rotate(0deg) scale(1); }
  50% { transform: rotate(180deg) scale(1.1); }
  100% { transform: rotate(360deg) scale(1); }
}

@keyframes pulse {
  0%, 100% { opacity: 0.6; }
  50% { opacity: 0.9; }
}

@keyframes dance {
  0%, 100% { transform: rotate(-10deg) translateX(0px); }
  25% { transform: rotate(10deg) translateX(-5px); }
  50% { transform: rotate(-10deg) translateX(5px); }
  75% { transform: rotate(10deg) translateX(-5px); }
}

/* Responsive adjustments */

/* iPhone X and similar (375px width) */
@media (max-width: 414px) {
  .app {
    padding: 5px;
    border-radius: 15px;
    border-width: 2px;
    min-height: 100vh;
  }
  
  .game-header {
    margin-bottom: 10px;
  }
  
  .players-info {
    gap: 20px;
    margin-bottom: 15px;
  }
  
  .player-info {
    padding: 8px 15px;
    font-size: 0.9rem;
  }
  
  .game-info {
    font-size: 1rem;
    margin-bottom: 15px;
  }
  
  .game-controls {
    gap: 10px;
    margin-top: 15px;
  }
  
  .reset-button,
  .mute-button {
    padding: 10px 20px;
    font-size: 1rem;
  }
  
  .room-code {
    padding: 10px 15px;
    margin-bottom: 15px;
  }
  
  .room-code .code {
    font-size: 1.5rem;
    letter-spacing: 2px;
    padding: 8px 15px;
  }
  
  .copy-button {
    padding: 6px 12px;
    font-size: 0.8rem;
  }
  
  .character {
    width: 40px !important;
    height: 40px !important;
  }
}

/* Tablets and small laptops */
@media (max-width: 768px) and (min-width: 415px) {
  .app {
    padding: 8px;
  }
  
  .character {
    width: 50px !important;
    height: 50px !important;
  }
}

/* Large screens */
@media (min-width: 1200px) {
  .app {
    padding: 30px;
  }
}

