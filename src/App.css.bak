@keyframes victoryFadeIn {
  from { opacity: 0; }
  to { opacity: 1; }
}

@keyframes defeatFadeIn {
  from { opacity: 0; }
  to { opacity: 1; }
}

@keyframes victoryBackground {
  0% { background-position: 0% 50%; }
  50% { background-position: 100% 50%; }
  100% { background-position: 0% 50%; }
}

@keyframes defeatBackground {
  0% { background-position: 0% 50%; }
  50% { background-position: 100% 50%; }
  100% { background-position: 0% 50%; }
}

@keyframes victoryTextSequence {
  0% { transform: scale(0); opacity: 0; }
  15% { transform: scale(1.2); opacity: 1; }
  25% { transform: scale(1); opacity: 1; }
  75% { transform: scale(1); opacity: 1; }
  100% { transform: scale(0.9); opacity: 0.9; }
}

@keyframes defeatTextSequence {
  0% { transform: scale(0); opacity: 0; }
  15% { transform: scale(1.2); opacity: 1; }
  25% { transform: scale(1); opacity: 1; }
  75% { transform: scale(1); opacity: 1; }
  100% { transform: scale(0.9); opacity: 0.9; }
}

.victory-text h1 { animation: victoryTextSequence 3s ease-in-out; }
.defeat-text h1 { animation: defeatTextSequence 3s ease-in-out; }

.victory-subtitle { animation: victorySubtitleSequence 3s ease-in-out; }
.defeat-subtitle { animation: defeatSubtitleSequence 3s ease-in-out; }

.victory-description { animation: victoryDescriptionSequence 3s ease-in-out; }
.defeat-description { animation: defeatDescriptionSequence 3s ease-in-out; }

@keyframes victorySubtitleSequence {
  0% { opacity: 0; transform: translateY(30px); }
  30% { opacity: 0; transform: translateY(30px); }
  50% { opacity: 1; transform: translateY(0); }
  100% { opacity: 1; transform: translateY(0); }
}

@keyframes defeatSubtitleSequence {
  0% { opacity: 0; transform: translateY(30px); }
  30% { opacity: 0; transform: translateY(30px); }
  50% { opacity: 1; transform: translateY(0); }
  100% { opacity: 1; transform: translateY(0); }
}

@keyframes victoryDescriptionSequence {
  0% { opacity: 0; transform: translateY(20px); }
  50% { opacity: 0; transform: translateY(20px); }
  70% { opacity: 1; transform: translateY(0); }
  100% { opacity: 1; transform: translateY(0); }
}

@keyframes defeatDescriptionSequence {
  0% { opacity: 0; transform: translateY(20px); }
  50% { opacity: 0; transform: translateY(20px); }
  70% { opacity: 1; transform: translateY(0); }
  100% { opacity: 1; transform: translateY(0); }
}

/* Responsive design for victory/defeat moments */
@media (max-width: 768px) {
  .victory-text h1, .defeat-text h1 {
    font-size: 3.5rem;
  }
  
  .victory-subtitle, .defeat-subtitle {
    font-size: 1.6rem;
  }
  
  .victory-description, .defeat-description {
    font-size: 1.2rem;
  }
}

@media (max-width: 480px) {
  .victory-text h1, .defeat-text h1 {
    font-size: 2.5rem;
  }
  
  .victory-subtitle, .defeat-subtitle {
    font-size: 1.3rem;
  }
  
  .victory-description, .defeat-description {
    font-size: 1rem;
  }
}

/* Advancing Transition */
.advancing-transition {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: linear-gradient(45deg, #1a1a2e, #16213e, #0f3460, #16213e, #1a1a2e);
  background-size: 400% 400%;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 1000;
  animation: advancingBackground 2.5s ease-in-out, advancingFadeIn 0.4s ease-in-out;
}

.advancing-text {
  text-align: center;
  animation: advancingTextSequence 2.5s ease-in-out;
}

.advancing-text h1 {
  font-family: 'Fredoka', 'Comic Sans MS', 'Marker Felt', 'Bradley Hand', cursive, sans-serif;
  font-size: 4.5rem;
  background: linear-gradient(45deg, #4169e1, #00bfff, #1e90ff);
  background-clip: text;
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  
  margin: 0;
  font-weight: bold;
  transform: scale(0);
}

.advancing-subtitle {
  font-family: 'Fredoka', 'Comic Sans MS', 'Marker Felt', 'Bradley Hand', cursive, sans-serif;
  font-size: 1.8rem;
  color: #b0c4de;
  
  margin-top: 20px;
  opacity: 0;
}

.advancing-description {
  font-family: 'Fredoka', 'Comic Sans MS', 'Marker Felt', 'Bradley Hand', cursive, sans-serif;
  font-size: 1.4rem;
  color: #b0c4de;
  
  margin-top: 15px;
  opacity: 0;
}

.opponent-name {
  color: #ff6b35;
  
  font-weight: bold;
}

@keyframes advancingFadeIn {
  from { opacity: 0; }
  to { opacity: 1; }
}

@keyframes advancingBackground {
  0% { background-position: 0% 50%; }
  50% { background-position: 100% 50%; }
  100% { background-position: 0% 50%; }
}

@keyframes advancingTextSequence {
  0% { transform: scale(0); opacity: 0; }
  20% { transform: scale(1.1); opacity: 1; }
  30% { transform: scale(1); opacity: 1; }
  80% { transform: scale(1); opacity: 1; }
  100% { transform: scale(0.95); opacity: 0.95; }
}

.advancing-text h1 { animation: advancingTextSequence 2.5s ease-in-out; }
.advancing-subtitle { animation: advancingSubtitleSequence 2.5s ease-in-out; }
.advancing-description { animation: advancingDescriptionSequence 2.5s ease-in-out; }

@keyframes advancingSubtitleSequence {
  0% { opacity: 0; transform: translateY(30px); }
  35% { opacity: 0; transform: translateY(30px); }
  55% { opacity: 1; transform: translateY(0); }
  100% { opacity: 1; transform: translateY(0); }
}

@keyframes advancingDescriptionSequence {
  0% { opacity: 0; transform: translateY(20px); }
  55% { opacity: 0; transform: translateY(20px); }
  75% { opacity: 1; transform: translateY(0); }
  100% { opacity: 1; transform: translateY(0); }
}

/* Responsive design for advancing transition */
@media (max-width: 768px) {
  .advancing-text h1 {
    font-size: 3.2rem;
  }
  
  .advancing-subtitle {
    font-size: 1.5rem;
  }
  
  .advancing-description {
    font-size: 1.2rem;
  }
}

@media (max-width: 480px) {
  .advancing-text h1 {
    font-size: 2.4rem;
  }
  
  .advancing-subtitle {
    font-size: 1.2rem;
  }
  
  .advancing-description {
    font-size: 1rem;
  }
}

/* Battle Interface Styles */
.battle-interface {
  margin-bottom: 30px;
}

.fighters-display {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 30px;
  max-width: 800px;
  margin: 0 auto;
}

.vs-divider {
  display: flex;
  flex-direction: column;
  align-items: center;
  gap: 10px;
  position: relative;
}

.vs-text {
  font-size: 2.5rem;
  font-weight: bold;
  color: #ffd700;
  text-shadow: 
    0 0 15px #ff4500,
    3px 3px 0px #660000,
    5px 5px 0px #330000;
  /* Static VS glow for performance */
  color: #ffd700;
  
  font-family: 'Fredoka', 'Comic Sans MS', 'Marker Felt', 'Bradley Hand', cursive, sans-serif;
}

@keyframes vsGlow {
  0% { 
    color: #ffd700;
    text-shadow: 
      0 0 15px #ff4500,
      3px 3px 0px #660000,
      5px 5px 0px #330000;
  }
  100% { 
    color: #ffff00;
    text-shadow: 
      0 0 25px #ff6b35,
      3px 3px 0px #660000,
      5px 5px 0px #330000;
  }
}

.battle-result {
  font-size: 1.5rem;
  font-weight: bold;
  color: #ff6b35;
  
  font-family: 'Fredoka', 'Comic Sans MS', 'Marker Felt', 'Bradley Hand', cursive, sans-serif;
  /* Static result text for performance */
  text-align: center;
}

@keyframes resultPulse {
  0%, 100% { transform: scale(1); }
  50% { transform: scale(1.1); }
}

.battle-status {
  font-size: 1.2rem;
  color: #ffccaa;
  
  font-family: 'Fredoka', 'Comic Sans MS', 'Marker Felt', 'Bradley Hand', cursive, sans-serif;
  /* Static status for performance */
  color: #ff6b35;
  text-align: center;
}

@keyframes statusGlow {
  0% { color: #ffccaa; }
  100% { color: #ff6b35; }
}

/* Responsive Battle Interface */
@media (max-width: 768px) {
  .fighters-display {
    flex-direction: column;
    gap: 20px;
  }
  
  .vs-divider {
    order: -1;
    margin-bottom: 10px;
  }
  
  .vs-text {
    font-size: 2rem;
  }
  
  .battle-result {
    font-size: 1.3rem;
  }
  
  .battle-status {
    font-size: 1rem;
  }
}

@media (max-width: 480px) {
  .vs-text {
    font-size: 1.5rem;
  }
  
  .battle-result {
    font-size: 1.1rem;
  }
  
  .battle-status {
    font-size: 0.9rem;
  }
}