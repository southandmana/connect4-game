/* Story Intro Transition */
.story-intro {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: black;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 1000;
  animation: storyIntroFadeIn 0.8s ease-in-out;
}


.story-text {
  position: relative;
  z-index: 1001;
  text-align: center;
}

.story-text h1 {
  font-family: 'Fredoka', 'Comic Sans MS', 'Marker Felt', 'Bradley Hand', cursive, sans-serif;
  font-size: 4rem;
  background: linear-gradient(45deg, #ff6b35, #ffa500, #ff4500);
  background-clip: text;
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  
  margin: 0;
  animation: storyTextSequence 2.5s ease-in-out forwards;
}

@keyframes storyIntroFadeIn {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}


@keyframes storyTextSequence {
  0% {
    opacity: 0;
    transform: scale(0.3);
  }
  20% {
    opacity: 1;
    transform: scale(1.2);
  }
  30% {
    opacity: 1;
    transform: scale(1);
  }
  70% {
    opacity: 1;
    transform: scale(1);
  }
  100% {
    opacity: 0;
    transform: scale(0.3);
  }
}

/* Responsive design for story text */
@media (max-width: 768px) {
  .story-text h1 {
    font-size: 2.5rem;
  }
}

@media (max-width: 480px) {
  .story-text h1 {
    font-size: 2rem;
  }
}

/* Battle Transition */
.battle-transition {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: linear-gradient(45deg, #1a0000, #4d0000, #800000, #4d0000, #1a0000);
  background-size: 400% 400%;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 1000;
  animation: battleBackground 2s ease-in-out, battleFadeIn 0.3s ease-in-out;
}

.battle-text {
  text-align: center;
  animation: battleTextSequence 2s ease-in-out;
}

.battle-text h1 {
  font-family: 'Fredoka', 'Comic Sans MS', 'Marker Felt', 'Bradley Hand', cursive, sans-serif;
  font-size: 6rem;
  background: linear-gradient(45deg, #ff0000, #ff4500, #ff6b35, #ff8c50);
  background-clip: text;
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  
  margin: 0;
  font-weight: bold;
  transform: scale(0);
}

.battle-subtitle {
  font-family: 'Fredoka', 'Comic Sans MS', 'Marker Felt', 'Bradley Hand', cursive, sans-serif;
  font-size: 1.8rem;
  color: #ffccaa;
  
  margin-top: 20px;
  opacity: 0;
}

@keyframes battleFadeIn {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}

@keyframes battleBackground {
  0% {
    background-position: 0% 50%;
  }
  50% {
    background-position: 100% 50%;
  }
  100% {
    background-position: 0% 50%;
  }
}

@keyframes battleTextSequence {
  0% {
    transform: scale(0);
    opacity: 0;
  }
  20% {
    transform: scale(1.3);
    opacity: 1;
  }
  30% {
    transform: scale(1);
    opacity: 1;
  }
  60% {
    transform: scale(1);
    opacity: 1;
  }
  70% {
    opacity: 1;
  }
  100% {
    transform: scale(0.9);
    opacity: 0.8;
  }
}

.battle-text h1 {
  animation: battleTextSequence 2s ease-in-out;
}

.battle-subtitle {
  animation: battleSubtitleSequence 2s ease-in-out;
}

@keyframes battleSubtitleSequence {
  0% {
    opacity: 0;
    transform: translateY(20px);
  }
  40% {
    opacity: 0;
    transform: translateY(20px);
  }
  60% {
    opacity: 1;
    transform: translateY(0);
  }
  100% {
    opacity: 1;
    transform: translateY(0);
  }
}

/* Responsive design for battle text */
@media (max-width: 768px) {
  .battle-text h1 {
    font-size: 4rem;
  }
  
  .battle-subtitle {
    font-size: 1.4rem;
  }
}

@media (max-width: 480px) {
  .battle-text h1 {
    font-size: 3rem;
  }
  
  .battle-subtitle {
    font-size: 1.2rem;
  }
}

